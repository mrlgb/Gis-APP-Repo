<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tt.rds.app">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <uses-feature
        android:glEsVersion="0x00020000"
        android:required="true" />

    <application
        android:name=".MainApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity.SearchActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity.DataQueryActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity.BridgeActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity.Bridge2Activity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".activity.FerryActivity" />
        <activity android:name=".activity.BoundaryPointActivity" />
        <activity android:name=".activity.TunnelActivity" />
        <activity android:name=".activity.SectionActivity" />
        <activity android:name=".activity.BegincollectingActivity" />
        <activity android:name=".activity.FinishcollectingActivity" />
        <activity android:name=".activity.point.PointMarkerActivity" />
        <activity
            android:name=".activity.WelcomActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.usersetting.PointSetActivity"
            android:theme="@style/AppTheme.NoActionBar">

        </activity>
        <activity
            android:name=".activity.usersetting.FeedbackActivity"
            android:theme="@style/AppTheme.NoActionBar">

        </activity>
    </application>

</manifest>